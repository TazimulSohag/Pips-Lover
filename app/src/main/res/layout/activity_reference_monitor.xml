<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ReferenceMonitorActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_refmon"
        app:title="Reference Monitor"
        android:background="@drawable/backgroundtwo"
        app:titleTextColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="50dp" />

    <ListView
        android:id="@+id/agent_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar_refmon"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="268dp" />



</RelativeLayout>